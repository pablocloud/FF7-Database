--
-- Estructura de tabla para la tabla `enemigos`
--

CREATE TABLE IF NOT EXISTS `enemigos` (
  `imagen` varchar(100) NOT NULL DEFAULT 'imagenes/enemigos/imagen.png',
  `nombre` varchar(100) NOT NULL,
  `nivel` int(100) NOT NULL,
  `pg` int(100) NOT NULL,
  `mp` int(100) NOT NULL,
  `exp` int(100) NOT NULL,
  `ap` int(100) NOT NULL,
  `gil` int(100) NOT NULL,
  `debil` varchar(100) NOT NULL,
  `fuerte` varchar(100) NOT NULL,
  `absorve` varchar(100) NOT NULL,
  `inmune` varchar(100) NOT NULL,
  `Jefe` set('Si','No') NOT NULL DEFAULT 'No'
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

